#include "traffic_light.h"

/*
 * Blink led red with frequency = 2Hz
 * input: none
 * output: none
 * */
void blinkRed();

/*
 * Blink led yellow with frequency = 2Hz
 * input: none
 * output: none
 * */
void blinkYellow();

/*
 * Blink led green with frequency = 2Hz
 * input: none
 * output: none
 * */
void blinkGreen();
