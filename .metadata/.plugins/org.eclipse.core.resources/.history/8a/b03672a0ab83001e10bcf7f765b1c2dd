#include "mode2.h"

MODE2_STATE state = INIT;

void mode2()
{
	switch (state)
	{
	case INIT:
		break;
	case INCREASE:
		break;
	case SET:
		break;
	// increase time duration
	// set time duration
	}


	// blink led red
	HAL_GPIO_TogglePin(GPIOA, V_RED_Pin|H_RED_Pin);
}
