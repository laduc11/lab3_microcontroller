#include "mode1.h"

/*
 * run mode 1 in state machine
 * input: current state
 * output: none
 * */
void mode1(TRAFFIC state)
{
	switch (state)
	{
	case INIT:
		HAL_GPIO_WritePin(GPIOA, H_RED, PinState)
		break;
	case INIT:
		break;
	case INIT:
		break;
	case INIT:
		break;
	case INIT:
		break;
	default:

	}
}
